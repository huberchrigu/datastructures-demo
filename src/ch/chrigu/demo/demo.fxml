<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="213.0" prefWidth="515.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.chrigu.demo.ui.DemoController" fx:id="borderPane">
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
             <Button fx:id="addCollectionButton" onAction="#addCollection" text="Add Collection" />
             <Button fx:id="removeCollectionButton" onAction="#removeCollection" text="Remove Collection" />
             <Button fx:id="clearCollectionsButton" onAction="#clearAllCollections" text="Clear All Collections" />
            <Button fx:id="clearCollectionsButton1" onAction="#clearAllCollections" text="Start Action" />
         </children>
      </HBox>
   </top>
   <center>
      <AnchorPane prefWidth="450.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="collectionsTable" prefHeight="100.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <columns>
                <TableColumn fx:id="collectionName" prefWidth="75.0" text="Collection" />
                <TableColumn fx:id="collectionParameters" prefWidth="75.0" text="Parameters" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
