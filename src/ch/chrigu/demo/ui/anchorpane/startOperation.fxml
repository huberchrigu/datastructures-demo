                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
              </HBox>
              <HBox>
                 <children>
                     <Label text="Data Set Generator">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Label>
