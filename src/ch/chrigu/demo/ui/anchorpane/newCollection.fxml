<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ch.chrigu.demo.ui.anchorpane.AddController">
    <VBox prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <ChoiceBox prefWidth="150.0" fx:id="type"/>
        <Label text="Initial capacity"/>
        <TextField fx:id="capacity"/>
        <HBox prefHeight="42.0" prefWidth="600.0">
            <CheckBox mnemonicParsing="false" text="Synchronized" fx:id="synchronizedCollection"/>
            <CheckBox mnemonicParsing="false" text="Blocking" fx:id="blocking"/>
        </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0">
            <Button mnemonicParsing="false" text="Add" onAction="#add"/>
            <Button mnemonicParsing="false" text="Cancel" onAction="#cancel"/>
        </HBox>
    </VBox>
</AnchorPane>
